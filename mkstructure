#!/bin/bash
mkdir "$HOME/Downloads/Arquivos de Texto"
mkdir "$HOME/Downloads/Torrents"
mkdir "$HOME/Downloads/Compactados"
mkdir "$HOME/Imagens/Imagens Baixadas"
mkdir "$HOME/.fonts"
mkdir "$HOME/Downloads/Executaveis"
mkdir "$HOME/Downloads/scripts baixados"
mkdir "$HOME/Downloads/Isos"
ln -s "$HOME/Downloads/Arquivos de Texto" "$HOME/Documentos/Documentos Baixados"
